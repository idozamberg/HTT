OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/DTCoreText" -isystem "${PODS_ROOT}/Headers/Public/DTFoundation" -isystem "${PODS_ROOT}/Headers/Public/ReaderFramework" -isystem "${PODS_ROOT}/Headers/Public/YIPopupTextView"
OTHER_LDFLAGS = -ObjC -l"Pods-DTCoreText" -l"Pods-DTFoundation" -l"Pods-ReaderFramework" -l"Pods-YIPopupTextView" -l"xml2" -framework "CoreGraphics" -framework "CoreText" -framework "ImageIO" -framework "MediaPlayer" -framework "QuartzCore"
PODS_ROOT = ${SRCROOT}/Pods